{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "ESNext"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["@netlify/functions"]
  },
  "include": [
    "src",
    "netlify/functions"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}